{
	"name": "@synthetixio/contracts-interface",
	"publishConfig": {
		"access": "public"
	},
	"version": "2.73.1-1",
	"description": "A library for interacting with Synthetix smart contracts",
	"source": "./src/index.ts",
	"main": "./build/node/src/index.js",
	"module": "./build/node/src/index.js",
	"browser": "./build/node/src/index.js",
	"types": "./build/node/src/index.d.ts",
	"files": [
		"build",
		"src"
	],
	"scripts": {
		"build": "node ./codegen.js && yarn build-browser && yarn build-node",
		"build-node": "tsc -p tsconfig.node.json",
		"build-browser": "webpack-cli --mode=production --max-old-space-size=4096",
		"examples:node": "ts-node --project tsconfig.node.json ./examples/signer-example.js",
		"examples:browser": "yarn build-browser && http-server -o ./examples/browser-example.html -c-1",
		"lint": "eslint './src/**/*.{js,ts,tsx}' && tsc",
		"lint:fix": "eslint --fix './src/**/*.{js,ts,tsx}'",
		"test": "jest --forceExit --coverage --no-cache",
		"watch": "yarn build-node && chokidar 'src/**/*.ts' -c 'yarn build-node'",
		"tsc": "tsc --noemit"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Synthetixio/js-monorepo"
	},
	"author": "Synthetix",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Synthetixio/js-monorepo/issues"
	},
	"dependencies": {
		"ethers": "^5.5.3",
		"synthetix": "v2.73.1"
	},
	"devDependencies": {
		"@babel/core": "^7.11.0",
		"@babel/plugin-transform-runtime": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@babel/preset-typescript": "^7.13.0",
		"@types/node": "^17.0.43",
		"chokidar-cli": "^2.1.0",
		"eslint": "^7.26.0",
		"jest": "^27.0.6",
		"ts-loader": "^8.0.2",
		"ts-node": "^10.2.1",
		"typescript": "^4.4.2",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"jest": {
		"testMatch": [
			"<rootDir>/**/*.test.{js,jsx,ts,tsx}"
		],
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.{js,jsx,ts,tsx}",
			"!<rootDir>/src/**/*.d.{js,jsx,ts,tsx}"
		],
		"coverageThreshold": {
			"global": {
				"branches": 70,
				"functions": 70,
				"lines": 80,
				"statements": 80
			}
		}
	},
	"stableVersion": "2.73.1"
}
